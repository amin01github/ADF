{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Aminn02adf"
		},
		"SQL_DB_LS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SQL_DB_LS'"
		},
		"Adls_LS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://amin01adls.dfs.core.windows.net/"
		},
		"Rest_LS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://restcountries.com/v3.1/all"
		},
		"Rest_param_LS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://restcountries.com/v3.1/name"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Aminn02adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"SQL_DB_LS_connectionString": {
						"value": "[parameters('SQL_DB_LS_connectionString')]"
					},
					"Adls_LS_properties_typeProperties_url": {
						"value": "[parameters('Adls_LS_properties_typeProperties_url')]"
					},
					"Rest_LS_properties_typeProperties_url": {
						"value": "[parameters('Rest_LS_properties_typeProperties_url')]"
					},
					"Rest_param_LS_properties_typeProperties_url": {
						"value": "[parameters('Rest_param_LS_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}